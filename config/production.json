{
  "@opentermsarchive/engine": {
    "recorder": {
      "versions": {
        "storage": {
          "git": {
            "snapshotIdentiferTemplate": "https://github.com/Ndpnt/test7-snapshots/commit/%SNAPSHOT_ID",
            "repository": "git@github.com:Ndpnt/test7-versions.git",
            "path": "./data/versions"
          }
        }
      },
      "snapshots": {
        "storage": {
          "git": {
            "repository": "git@github.com:Ndpnt/test7-snapshots.git",
            "path": "./data/snapshots"
          }
        }
      }
    },
    "fetcher": {
      "language": "en"
    },
    "notifier": {
      "sendInBlue": {
        "updatesListId": 0,
        "updateTemplateId": 0
      }
    },
    "reporter": {
      "githubIssues": {
        "repositories": {
          "declarations": "Ndpnt/test7-declarations",
          "versions": "Ndpnt/test7-versions",
          "snapshots": "Ndpnt/test7-snapshots"
        }
      }
    },
    "dataset": {
      "title": "test7",
      "versionsRepositoryURL": "https://github.com/Ndpnt/test7-versions"
    },
    "collection-api": {
      "port": 3000,
      "basePath": "/collection-api"
    },
    "logger": {
      "timestampPrefix": false
    }
  }
}
